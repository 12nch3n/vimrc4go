export tLANG="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"


tmux_init()
{
	if  [ ! -z "$TMUX" ]; then
		tmux has_session -t IDE
		if [ $? != 0 ]; then 
			tmux new-session -s "IDE" -d -n "GOVIM"
			tmux splitw -t IDE:1 -d -v -p 10
			tmux splitw -t IDE:2 -d -v -p 10
			tmux -2 attach-session -d
			tmux select-window -t IDE:2
		else
			tmux attach -t IDE
		fi
	fi
}
